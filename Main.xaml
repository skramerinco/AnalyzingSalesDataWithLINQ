  <Sequence sap2010:Annotation.AnnotationText="Analyzing Sales Data steps:&#xA;- Read Excel file SalesData.xlsx&#xA;- Use LINQ to order table rows by Total Sales ASC&#xA;- Use LINQ to verify if Total Sales &gt; Target Sales for any of the table rows&#xA;- Use LINQ  to find MAX Total Sales for EMEA departments&#xA;- Use LINQ to build a dictionary from the first two columns of the datatable" DisplayName="Main Sequence" sap:VirtualizedContainerService.HintSize="496,1948.8" sap2010:WorkflowViewState.IdRef="Sequence_1">
    <Sequence.Variables>
      <Variable x:TypeArguments="sd:DataTable" Name="dt_SalesData" />
      <Variable x:TypeArguments="x:String" Name="strSalesData" />
      <Variable x:TypeArguments="x:Boolean" Name="AnyTargetSalesReached" />
      <Variable x:TypeArguments="x:Int32" Name="EmeaMaxSales" />
      <Variable x:TypeArguments="scg:Dictionary(x:String, x:String)" Name="SalesDictionary" />
    </Sequence.Variables>
    <sap:WorkflowViewStateService.ViewState>
      <scg:Dictionary x:TypeArguments="x:String, x:Object">
        <x:Boolean x:Key="IsExpanded">True</x:Boolean>
        <x:Boolean x:Key="IsAnnotationDocked">True</x:Boolean>
      </scg:Dictionary>
    </sap:WorkflowViewStateService.ViewState>
    <ui:ReadRange Range="{x:Null}" AddHeaders="True" DataTable="[dt_SalesData]" DisplayName="Read Range Workbook - Sales Data" sap:VirtualizedContainerService.HintSize="433.6,88" sap2010:WorkflowViewState.IdRef="ReadRange_1" SheetName="Sheet1" WorkbookPath="SalesData.xlsx" />
    <Assign DisplayName="Assign - LINQ to order the table rows by &quot;Total Sales&quot; column ASC" sap:VirtualizedContainerService.HintSize="433.6,81.6" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="sd:DataTable">[dt_SalesData]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable">[dt_SalesData.AsEnumerable.OrderBy(Function(row) row("Total Sales")).CopyToDataTable]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:OutputDataTable DataTable="[dt_SalesData]" DisplayName="Output Data Table" sap:VirtualizedContainerService.HintSize="433.6,80.8" sap2010:WorkflowViewState.IdRef="OutputDataTable_1" Text="[strSalesData]" />
    <ui:LogMessage DisplayName="Log ordered sales data" sap:VirtualizedContainerService.HintSize="433.6,142.4" sap2010:WorkflowViewState.IdRef="LogMessage_1" Message="[strSalesData]" />
    <Assign DisplayName="Assign - LINQ to find if any of the sales departments have exceeded Taget Sales" sap:VirtualizedContainerService.HintSize="433.6,81.6" sap2010:WorkflowViewState.IdRef="Assign_2">
      <Assign.To>
        <OutArgument x:TypeArguments="x:Boolean">[AnyTargetSalesReached]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:Boolean">[dt_SalesData.AsEnumerable.Any(Function (x) Convert.ToInt32(x("Total Sales"))&gt;Convert.ToInt32(x("Target Sales")))]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:LogMessage DisplayName="Log if any sales targets where reached" sap:VirtualizedContainerService.HintSize="433.6,142.4" sap2010:WorkflowViewState.IdRef="LogMessage_2" Message="[&quot;Sales targets where reached: &quot; + AnyTargetSalesReached.ToString]" />
    <Assign DisplayName="Assign - LINQ to find the maximum for Sales for EMEA group" sap:VirtualizedContainerService.HintSize="433.6,81.6" sap2010:WorkflowViewState.IdRef="Assign_3">
      <Assign.To>
        <OutArgument x:TypeArguments="x:Int32">[EmeaMaxSales]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:Int32">[dt_SalesData.AsEnumerable.where(Function(row) row("Sales Department").ToString.Contains("EMEA")).AsEnumerable.Max(Function (x) Convert.ToInt32(x("Total Sales")))]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:LogMessage DisplayName="Log Message" sap:VirtualizedContainerService.HintSize="433.6,142.4" sap2010:WorkflowViewState.IdRef="LogMessage_3" Message="[&quot;Max EMEA Sales: &quot; + EmeaMaxSales.ToString]" />
    <Assign DisplayName="Assign - instantiate dictionary" sap:VirtualizedContainerService.HintSize="433.6,81.6" sap2010:WorkflowViewState.IdRef="Assign_4">
      <Assign.To>
        <OutArgument x:TypeArguments="scg:Dictionary(x:String, x:String)">[SalesDictionary]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="scg:Dictionary(x:String, x:String)">[new Dictionary(of String, String) from {}]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Assign - dept and total sale to dictionary" sap:VirtualizedContainerService.HintSize="433.6,81.6" sap2010:WorkflowViewState.IdRef="Assign_5">
      <Assign.To>
        <OutArgument x:TypeArguments="scg:Dictionary(x:String, x:String)">[SalesDictionary]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="scg:Dictionary(x:String, x:String)">[dt_SalesData.AsEnumerable.Where(Function (x) Not String.IsNullOrEmpty(x("Sales Department").ToString)).ToDictionary(function ( k) k("Sales Department").ToString, function(j) j("Total Sales").ToString)]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:LogMessage DisplayName="Log dictionary keys" sap:VirtualizedContainerService.HintSize="433.6,142.4" sap2010:WorkflowViewState.IdRef="LogMessage_4" Message="[&quot;Dictionary keys: &quot;+String.join (&quot;; &quot;,SalesDictionary.keys)]" />
    <ui:LogMessage DisplayName="Log dictionary values" sap:VirtualizedContainerService.HintSize="433.6,142.4" sap2010:WorkflowViewState.IdRef="LogMessage_5" Message="[&quot;Dictionary values: &quot;+String.join (&quot;; &quot;,SalesDictionary.values)]" />
